<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.dao.ProductDao">

    <!--
    符号	替换符号
    >	    &gt;
    >=	&gt;=
    <	    &lt;
    <=	&lt;=
    &	&amp;
    '	&apos;
    "	&quot;
      -->
    <resultMap id="BaseResultMap" type="org.example.entity.Product">
        <id column="product_id" jdbcType="INTEGER" property="id"/>
        <result column="product_name" jdbcType="VARCHAR" property="name"/>
        <result column="product_num" jdbcType="INTEGER" property="num"/>
    </resultMap>

</mapper>