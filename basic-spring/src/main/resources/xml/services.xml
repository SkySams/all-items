<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd" >

    <bean id="petStoreService" class="com.example.basicspring.service.impl.PetStoreServiceImpl">
    </bean>

    <bean id="clientService"
          class="com.example.basicspring.service.ClientService"
          factory-method="createInstance"/>

    <beans>
        <bean id="something" class="com.example.basicspring.service.SomeClass">
            <property name="accounts">
                <map>
                    <entry key="one" value="9.99"/>
                    <entry key="two" value="2.75"/>
                    <entry key="six" value="3.99"/>
                </map>
            </property>
        </bean>
    </beans>

</beans>